    "@rollup/plugin-typescript": "^12.1.2",
    "@types/react": "^19.0.2",
    "eslint": "^9.16.0",
    "prettier": "^3.2.5",
    "react": "^19.2.3",
    "rollup": "^4.29.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.1",
    "@types/react-dom": "^19.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
