    "@types/react": "^19.0.2",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "rollup": "^4.29.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
